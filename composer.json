{
    "name": "egeloen/json-builder",
    "description": "JSON builder with escaping control for PHP 5.6+",
    "keywords": [ "json", "builder", "escape" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "symfony/property-access": "^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^8.0",
        "symfony/phpunit-bridge": "^4.0"
    },
    "autoload": {
        "psr-4": { "Ivory\\JsonBuilder\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Ivory\\Tests\\JsonBuilder\\": "tests/" }
    }
}
